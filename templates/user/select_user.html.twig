{% extends 'base.html.twig' %}

{% block title %}Select a User{% endblock %}

{% block body %}
    <h1>Select a User</h1>

    <ul>
        {% for user in users %}
            <li>
                <a href="{{ path('app_dialog', {id: user.id}) }}">
                    {{ user.username }}
                </a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}